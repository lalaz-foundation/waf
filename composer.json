{
  "name": "lalaz/waf",
  "description": "Web Application Firewall with rate limiting, security headers, and CORS.",
  "type": "library",
  "license": "MIT",
  "require": {
    "php": "^8.3",
    "lalaz/framework": "^1.0.0-rc.1"
  },
  "require-dev": {
    "phpunit/phpunit": "^11.0"
  },
  "autoload": {
    "psr-4": {
      "Lalaz\\Waf\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Lalaz\\Waf\\Tests\\": "tests/"
    }
  },
  "minimum-stability": "dev",
  "prefer-stable": true
}
